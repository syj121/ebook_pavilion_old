<!-- 搜索参数 -->
<%% search_columns =   <%= attributes.map { |attribute| {type: :cont, column_name: attribute.name.to_sym} }%>%>

<%%= search_form_for q, :url => <%= singular_table_name.pluralize%>_path, :html => {:method => :get, :class => "well form-inline form-search"}  do |f| %>
  
  <% search_columns.each do |field_hash| %>
    <%= render "shared_templates/search", form: form, field_hash: field_hash%>
  <% end -%>

  <%= render "shared/button_search", form: form%>
<%% end %>


